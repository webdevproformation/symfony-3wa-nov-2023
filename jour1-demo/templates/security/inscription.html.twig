{% extends "base.html.twig" %}

{% block body %}

<h1>Inscription</h1>

{# {{ form(form) }} #}

{{  form_start(form)}}

<div class="row">
    <div class="offset-2 col-3">
        {{ form_row(form.email) }}
    </div>
    <div class="col-3">
        {{ form_row(form.password) }}
    </div>
    <div class="col-3">
        {{ form_row(form.pseudo) }}
    </div>
</div>
<div class="text-center">
        <input type="submit" class="btn btn-primary" >
    </div>
{{  form_end(form)}}
{% endblock %}